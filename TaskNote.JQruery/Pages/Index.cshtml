@page
@model TaskNote.JQruery.Pages.TaskListModel
@{
    var title = "TaskList";
    ViewData["Title"] = title;
}

<script src="~/js//task-list.js"></script>
<script src="~/js/api-client/base.js"></script>
<script src="~/js/api-client/task-api-client.js"></script>

<input type="button" value="追加" onclick="onAddTask();" />

<ul id="ul">
    @foreach (var task in Model.TaskShorts)
    {
        <li>
            <partial name="Moleceles/TaskItem.cshtml" suffix="@task.Id" model='@task' />
        </li>
    }

</ul>

<form method="post" asp-page="Sample" >
    <input type="text" name="userid" value="1" />
    <input type="text" name="email" value="2"/>
    <button type="submit" >ボタン</button>
</form>

